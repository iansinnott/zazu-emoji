    "eslint-config-zen": "^2.0.3",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.9.0",
    "eslint-plugin-standard": "^2.0.1"
  },
  "dependencies": {
    "debug": "^2.6.0",
    "emojilib": "^2.0.6",
